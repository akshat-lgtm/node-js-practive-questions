function Child({ user, fruits, click }) {
  return (
    <div>
      <h1>{user}</h1>

      <ul>
        {fruits.map((fruit, index) => (
          <li key={index}>{fruit}</li>
        ))}
      </ul>

      <button onClick={click}>Click Me</button>

      <div
        style={{
          margin: 10,
          width: 100,
          height: 100,
          backgroundColor: "blue"
        }}
      ></div>
    </div>
  );
}

export default Child;
